<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="stroke_style">
        <item name="android:layout_width">10dp</item>
        <item name="android:layout_height">18dp</item>
        <item name="android:layout_marginEnd">22dp</item>
        <item name="android:background">@drawable/ic_stroke</item>
    </style>

    <style name="settings_tvs_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">23dp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="fragment_name_ts">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">26dp</item>
        <item name="android:layout_marginStart">24dp</item>
    </style>

    <style name="btn_finish_text">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/cardview_dark_background</item>
    </style>
    <style name="difficulty">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/cardview_dark_background</item>
    </style>

    <style name="testStyleResultToolbar">
        <item name="android:textSize">17sp</item>
v    </style>
</resources>